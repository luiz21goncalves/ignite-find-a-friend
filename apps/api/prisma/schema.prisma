generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model State {
  id          String   @id @default(uuid())
  name        String
  acronym     String
  external_id String   @unique
  created_at  DateTime @default(now())
  updated_at  DateTime @default(now()) @updatedAt

  @@map("states")
}
